#include <iostream>
using std::cin, std::cout;
#include <cmath>

double f(int n) { return n*n+1; }

double f2(int n) {return abs(n);}

#define CATCH_CONFIG_MAIN
#include "catch2/catch_all.hpp"

TEST_CASE( "test that f always returns positive" ) {
  int n = 5;
  REQUIRE( f2(n) > 0 );
  int o=-1;
  REQUIRE( f2(o)>0 );
}
